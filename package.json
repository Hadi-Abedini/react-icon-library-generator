{
  "name": "react-icon-generator-cli",
  "description": "A fully typed, tree-shakable React icon library built with react-icon-generator-cli.",
  "private": false,
  "version": "1.0.7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hadi-Abedini/react-icon-library-generator.git"
  },
  "type": "module",
  "bin": {
    "react-icon-gen": "./bin/cli.ts"
  },
  "files": [
    "bin/",
    "scripts/",
    "utils/"
  ],
  "keywords": [
    "react",
    "typescript",
    "icon-generator",
    "svg-to-react",
    "cli",
    "tree-shaking",
    "asset-to-component"
  ],
  "scripts": {
    "build": "echo 'No build needed, Bun/TS runs directly'",
    "prepare": "bun run build",
    "generate:components": "bun scripts/generate-components.ts",
    "generate:index": "bun scripts/generate-index.ts",
    "generate:iconmap": "bun scripts/generate-iconmap.ts",
    "generate:preview": "bun scripts/generate-preview.ts",
    "generate": "bun run generate:components && bun run generate:index && bun run generate:iconmap && bun run generate:preview"
  },
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "prettier": "^3.7.4"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
